{
  "$schema": "http://json-schema.org/schema#",
  "additionalProperties": false,
  "id": "https://www.github.com/MISP/misp-warninglists/schema.json",
  "properties": {
    "description": {
      "type": "string"
    },
    "list": {
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "matching_attributes": {
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "name": {
      "type": "string"
    },
    "type": {
      "enum": [
        "string",
        "substring",
        "hostname",
        "cidr",
        "regex",
        "wildmask"
      ],
      "type": "string"
    },
    "version": {
      "type": "integer"
    }
  },
  "required": [
    "list",
    "description",
    "version",
    "name",
    "type"
  ],
  "title": "Validator for misp-warninglists",
  "type": "object"
}
